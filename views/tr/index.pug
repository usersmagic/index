            input.join-waitlist-input#name-input(type="text" autocomplete="none" placeholder="Ad Soyad")
          .join-waitlist-input-wrapper
            input.join-waitlist-input#company-name-input(type="text" autocomplete="none" placeholder="Şirket Adı")
            input.join-waitlist-input#company-url-input(type="text" autocomplete="none" placeholder="Şirket Websitesi")
